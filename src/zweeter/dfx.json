{
  "canisters": {
    "invoice_mock": {
      "dependencies": ["ledger"],
      "main": "src/mocks/invoice.mo",
      "type": "motoko"
    },
    "invoice": {
      "dependencies": ["ledger"],
      "main": "src/invoice/main.mo",
      "type": "motoko"
    },
    "ledger": {
      "type": "custom",
      "candid": "src/ledger/ledger.did",
      "wasm": "src/ledger/ledger.wasm"
    },
    "seller": {
      "type": "motoko",
      "main": "src/seller/main.mo"
    },
    "zweeter": {
      "main": "src/zweeter/main.mo",
      "type": "motoko"
    },
    "zweeter_assets": {
      "dependencies": ["zweeter"],
      "frontend": {
        "entrypoint": "src/zweeter_assets/src/index.html"
      },
      "source": ["src/zweeter_assets/assets", "dist/zweeter_assets/"],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "dfx": "0.8.5",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
